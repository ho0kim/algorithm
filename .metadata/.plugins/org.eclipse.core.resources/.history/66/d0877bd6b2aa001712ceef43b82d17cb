package beakjun;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] fibo = new int[40];
		fibo[0]=0;
		fibo[1]=1;
		for(int i=2;i<40;i++) {
			fibo[i]=fibo[i-1]+fibo[i+2];
		}
		
		for(int i =0;i<40;i++)
			System.out.println(fibo[i]);
		

	}


}
